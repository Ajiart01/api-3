  <link rel='stylesheet' href='https://cdn-uicons.flaticon.com/uicons-regular-rounded/css/uicons-regular-rounded.css'>
  
  <link rel='stylesheet' href='https://cdn-uicons.flaticon.com/uicons-solid-rounded/css/uicons-solid-rounded.css'>

  <style>
    body {
      font-family: 'Arial', sans-serif;
      margin: 0;
      padding: 0;
      background-color: #fff;
      color: #000;
      transition: background-color 0.3s ease, color 0.3s ease;
      padding-bottom: 100%;
      min-height: 2000px;
    } 

    .navbar {
      display: flex;
      align-items: center;
      justify-content: space-between;
      background-color: #ccc;
      padding: 10px 20px;
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      z-index: 9999;
      transition: background-color 0.3s ease;
    }

    .navbar .dark-mode-btn {
      margin-right: 25px;
      color: #000;
      font-size: 24px;
      cursor: pointer;
      transition: color 0.3s ease;
    }

    .navbar.dark-mode {
      background-color: #333;
      color: #000;
    }

    .navbar.dark-mode .menu-btn,
    .navbar.dark-mode .notification-btn,
    .navbar.dark-mode .dark-mode-btn,
    .navbar.dark-mode .chat-btn {
      color: #fff;
    }

    .navbar .menu-btn,
    .navbar .chat-btn,
    .navbar .notification-btn {
      color: #000;
      font-size: 24px;
      cursor: pointer;
      transition: color 0.3s ease;
      margin-right: 10px;
    }

    .navbar .right-icons {
      display: flex;
      align-items: center;
      gap: 10px;
    }

    .dropdown {
      position: relative;
      display: inline-block;
    }

    .dropdown-content {
      display: none;
      position: absolute;
      top: 30px;
      right: 0;
      min-width: 200px;
      background-color: #f9f9f9;
      box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
      padding: 12px 16px;
      z-index: 9999;
      color: #333;
    }

    .dropdown.dark-mode .dropdown-content {
      background-color: #333;
      color: #fff;
    }

    .dropdown:hover .dropdown-content {
      display: block;
    }

    #chat-box {
      position: center;
      bottom: 100px;
      right: 20px;
      width: 600px;
      height: 600px;
      background-color: rgba(0, 0, 0, 0.9);
      border-radius: 0;
      display: none;
      z-index: 9999;
    }

    #chat-box {
      width: 100%;
      height: 500px;
      margin-top: auto;
      margin-left: 10px;
      margin-right: 10px;
      bottom: 0;
      right: 0;
    }

    .chatbox-frame {
      align-items: center;
      width: 300px;
      height: 470px;
      border: none;
    }

    .dark-mode-icon {
      font-size: 24px;
      margin-right: 10px;
    }

    .info-boxes {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 20px;
  margin-top: 10px;
  padding: 20px;
  z-index: 0;
  margin-bottom: 50px; /* Tambahkan margin-bottom untuk menghindari tumpang tindih dengan footer */
}

    .info-box {
      background-color: #fff;
      border: 2px solid #ccc;
      border-radius: 8px;
      padding: 20px;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      text-align: center;
      max-width: 400px;
      width: 80%;
      color: #333;
      transition: background-color 0.3s ease, color 0.3s ease;
    }

    .info-box.dark-mode {
      background-color: #333;
      color: #fff;
    }

    .sidebar {
  position: fixed;
  top: 40px;
  left: 0;
  width: 0;
  height: 100%;
  background-color: #ccc;
  color: #000;
  transition: width 0.3s ease;
  overflow: hidden;
  overflow-y: auto;
  z-index: 1;
  margin-bottom: 50px; /* Tambahkan margin-bottom untuk menghindari tumpang tindih dengan footer */
}

    .sidebar.show {
      width: 50%;
      z-index: 2;
    }

    .sidebar .menu {
      padding: 20px;
    }

    .sidebar .menu a {
      display: block;
      color: #000;
      padding: 2.5px;
      text-decoration: none;
      transition: color 0.3s ease;
    }

    .sidebar .menu a:hover {
      color: #4CAF50;
    }

    .sidebar.dark-mode {
      background-color: #333;
      color: #fff;
    }

    .sidebar.dark-mode .menu a {
      color: #fff;
    }

    /* Responsif */
    @media (max-width: 768px) {
      .navbar .menu-btn,
      .navbar .chat-btn,
      .navbar .notification-btn,
      .navbar .dark-mode-btn {
        font-size: 20px;
      }

      .sidebar.show {
        width: 60%;
        z-index: 2;
      }
    }

    .dropdown-btn {
      width: 100%;
      border: none;
      background-color: #fff;
      color: #000;
      padding: 0;
      font-size: 16px;
      cursor: pointer;
    }

    .dropdown-content {
      display: none;
      padding-left: 16px;
    }

    .dropdown-content a {
      display: block;
    }

    .show {
      display: block;
    }

    /* Dark Mode */
    .dark-mode {
      background-color: #111;
      color: #fff;
    }

    .dark-mode .dropdown-content {
      background-color: #333;
      color: #fff;
    }

    .profile {
      display: flex;
      align-items: center;
    }

    .profile-info {
      margin-right: 10px;
    }

    .profile-info h3 {
      margin-right: 5px;
    }

    .profile-image {
      width: 20px;
      height: 20px;
      border: 1.5px solid #000;
      border-radius: 3px;
      flex-shrink: 0;
      flex-grow: 0;
      margin-right: 5px;
    }
    
 .table-container {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 5px;
      max-height: 300px; /* Atur tinggi maksimum tabel yang Anda inginkan */
  overflow-y: auto;
    }

    table {
      width: 100%;
      border-collapse: collapse;
      border: 1px solid #ccc;
    }

    th, td {
      border: 1px solid #ccc;
      padding: 20px;
      text-align: center;
    }

    @media screen and (max-width: 600px) {
      .table-container {
        grid-template-columns: 1fr;
      }
    }

 footer {
   position: fixed;
   bottom: 0;
   left: 0;
   width: 100%;
   background-color: transparent;
   text-align: center;
   z-index: 0;
 }
 
     .status-button {
      display: inline-block;
      padding: 10px 20px;
      font-size: 16px;
      background-color: #4CAF50;
      color: #fff;
      border: none;
      border-radius: 5px;
      cursor: pointer;
      position: relative;
      overflow: hidden;
      transition: color 0.3s ease, background-color 0.3s ease;
    }

    .status-button:hover {
      color: #fff;
      background-color: #4CAF50;
      box-shadow: 0 0 10px rgba(76, 175, 80, 0.5);
      /* Menerapkan efek brightness untuk kontras lebih tinggi saat di-hover */
      filter: brightness(120%);
    }

.action {
      display: inline-block;
      padding: 10px 20px;
      font-size: 16px;
      background-color: #33cccc;
      color: #fff;
      border: none;
      border-radius: 5px;
      cursor: pointer;
      position: relative;
      overflow: hidden;
      transition: color 0.3s ease, background-color 0.3s ease;
    }

    .action:hover {
      color: #fff;
      background-color: #4CAF50;
      box-shadow: 0 0 10px rgba(76, 175, 80, 0.5);
      /* Menerapkan efek brightness untuk kontras lebih tinggi saat di-hover */
      filter: brightness(120%);
    }

    .action:before {
      content: "";
      position: absolute;
      top: -5px;
      left: -5px;
      right: -5px;
      bottom: -5px;
      border-radius: 10px;
      background: linear-gradient(to bottom right, #33cccc, #4CAF50);
      z-index: -1;
    }
  </style>
